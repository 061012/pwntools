#!/usr/bin/env python2
import sys, re

try:
    if len(sys.argv) == 1:
        s = sys.stdin.read()
        s = re.sub(r'\s', '', s)
        sys.stdout.write(s.decode('hex'))
    else:
        sys.stdout.write(''.join(sys.argv[1:]).decode('hex'))
except TypeError, e:
    sys.stderr.write(str(e) + '\n')
